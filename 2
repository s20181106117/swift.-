//数组
var someInts:[Int] = [10,20,30,40,50]
var he=0
for item in someInts{
    he+=item
}
print(he)
var a=20
let b = 100
var c:Float = 20.0
var d = "\(a)乘于\(b)=\(c*100)"
print(d)
var array:[Int] = [2,9,80,70,67,45,34]
var qiuhe = 0
for o in array{
    qiuhe+=o
}
print(qiuhe)
var some = [Int]()
some.append(10)
some.append(20)
some+=[30]
for it in some
{
    print(it)
}
